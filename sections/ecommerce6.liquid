<div class="container-fluid bg-container">
        <div class="container my-50">
          <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 d-flex justify-content-between">
            <div class="text-left col-lg-6 col-md-6 col-sm-6 col-xs-6">
            <h1 class="font-weight-bold" >{{ section.settings.ecommerce6-title1}}</h1>
          </div>
          <div class="align-items-center text-right d-flex col-lg-6 col-md-6 col-sm-6 col-xs-6 justify-content-end d-flex" >
            <small class="text-right "><u class="text-right ">{{ section.settings.ecommerce6-button-top}}</u></small>
          </div>
        </div>
          </div>

          <div class="row d-flex justify-content-center">

            <div class="e6slider" id="wrapper6">
              <div><img src="{{ section.settings.eCard6-img1 | img_url: 'large' }}" alt="Product Image" class="image"></div>

              <div><img src="{{ section.settings.eCard6-img2 | img_url: 'large' }}" alt="Product Image" class="image"></div>

              <div><img src="{{ section.settings.eCard6-img3 | img_url: 'large' }}" alt="Product Image" class="image"></div>

            </div>

          


          </div>

   
</div>

</div>




      
      {% schema %}
{
  "name": "ecommerce 6",
	"max_blocks": 3,
  "settings": [
  {
    "id": "ecommerce6-title1",
    "type": "text",
    "label": "Ecommerce Title",
    "default": "Title"
  },
  {
    "id": "ecommerce6-button-top",
    "type": "text",
    "label": "button top",
    "default": "see all"
  },
  {
    "type": "image_picker",
    "id": "eCard6-img1",
    "label": "eCard Image 1"
  },
  {
    "type": "image_picker",
    "id": "eCard6-img2",
    "label": "eCard Image 2"
  },
  {
    "type": "image_picker",
    "id": "eCard6-img3",
    "label": "eCard Image 3"
  },
  {
    "type": "image_picker",
    "id": "eCard6-img4",
    "label": "eCard Image 4"
  },
  {
    "type": "image_picker",
    "id": "eCard6-img5",
    "label": "eCard Image 5"
  }
  ],

  "blocks": [
    {
      "type": "select",
      "name": "Add New Section",
      "settings": [
       
      ]
    }
  ],
  "presets": [
    {
      "name": "eccommerce 6",
      "category": "Blog",
      "blocks": [
        { 
          "type": "select"
        },
        {
          "type": "select"
        }
       
      ]
    }
  ]
}
{% endschema %}

{% javascript %}

  $(document).ready(function(){
      $('.e6slider').slick({
            dots: true,
            arrows: false,
            autoplay: true,
            autoplaySpeed: 1000
      });

  });

  
{% endjavascript %}