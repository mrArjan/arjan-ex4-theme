    "label": "eCard Image "
  },
  {
    "id": "ecommerce10-title1",
    "type": "text",
    "label": "Title",
    "default": "collection"
  },
  {
    "id": "ecommerce10-subtitle1",
    "type": "text",
    "label": "Card Subtitle",
    "default": "Summer 2019"
  },
  {
    "id": "ecommerce10-button1",
    "type": "text",
    "label": "Card Button",
    "default": "Button"
  },

  {
    "type": "image_picker",
    "id": "ecard10-img2-main",
    "label": "eCard Image "
  },
  {
    "id": "ecommerce10-title2",
    "type": "text",
    "label": "Title",
    "default": "collection"
  },
  {
    "id": "ecommerce10-subtitle2",
    "type": "text",
    "label": "Card Subtitle",
    "default": "Summer 2019"
  },
  {
    "id": "ecommerce10-button2",
    "type": "text",
    "label": "Card Button",
    "default": "Button"
  }
  ],

  "blocks": [
