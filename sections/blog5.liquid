<div class="container-fluid bg-container mt-5 blog5">
    <div class="container">
        <div class="text-left">
            <h1 class="font-weight-bold animated bounce delay-2s">
                    {{ section.settings.title-text-blog5}}
            </h1>
        </div>

        <div class=" row d-flex justify-content-center">
            <div class="col-lg-7 col-md-7 col-sm-5 col-xs-12 d-flex flex-column">
                
                    <div class="card mb-2 d-flex justify-content-center m-lg-4 m-md-1">
                      <div class="card-wrap d-flex justify-column">
                        <div class="card-img-top col-lg-6 col-md-6 col-sm-6  p-0 flex-sm-wrap"> <img src="{{ section.settings.card-img1 | img_url: 'large' }}" alt=""></div>
                        <div class="card-body">
                            <div class="card-label1"><h5 class="card-label">{{ section.settings.blog5-label1 }}</h5></div>
                          <div class="card-title">
                            <h4 class="font-weight-bold">
                                {{ section.settings.blog5-title1 }}
                            </h4>
                          </div>
                          <div class="card-text" style="overflow: hidden;">
                            <h5>{{ section.settings.blog5-description1 }}</h5>
                          </div>

                          <div class="card-button"><h5><u class="text-muted">See all</u></h5></div>
                        </div>
                      </div>
                    </div>
               

                    
                    <div class="card mb-2 d-flex justify-content-center m-lg-4 m-md-1">
